-- Mux 2 para 1 em VHDL
-- Entradads: Ea, Eb, SEL
-- Saídas: Saida
--
-- SEL Saida
-- ------------
-- 0    Ea
-- 1    Eb
--
-- Estudante: Kaike C. 
-- 24/02/2020

entity mux2x1 is
  
  port( Ea, Eb, SEL: in bit;
    
        Saida : out bit);
end;

architecture ckt_mux2x1 of mux2x1 is
  
 begin
  
  Saida <= (Ea and not(SEL)) or ( Eb and (SEL));
  
end ckt_mux2x1;
