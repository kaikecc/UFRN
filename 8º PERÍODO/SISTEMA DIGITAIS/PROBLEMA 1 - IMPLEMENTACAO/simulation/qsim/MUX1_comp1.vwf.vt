module MUX_comp1_vlg_sample_tst(
	CT_Msg,
	CT_ON,
	DISPLAY_VAL,
	REG_GRV,
	REGS_REP_S,
	REGS_REP_SQ,
	s1_comp1,
	s2_comp1,
	s_comp1,
	sampler_tx
);
input [7:0] CT_Msg;
input [7:0] CT_ON;
input [7:0] DISPLAY_VAL;
input [7:0] REG_GRV;
input [7:0] REGS_REP_S;
input [7:0] REGS_REP_SQ;
input  s1_comp1;
input  s2_comp1;
input  s_comp1;
output sampler_tx;

reg sample;
time current_time;
always @(CT_Msg or CT_ON or DISPLAY_VAL or REG_GRV or REGS_REP_S or REGS_REP_SQ or s1_comp1 or s2_comp1 or s_comp1)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

